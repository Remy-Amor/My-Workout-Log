
<Application x:Class="MyWorkoutLog.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MyWorkoutLog"
             xmlns:viewModels="clr-namespace:MyWorkoutLog.MVVM.ViewModels"
             xmlns:view="clr-namespace:MyWorkoutLog.view">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:AccountViewModel}">
            <view:Account/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:HistoryViewModel}">
            <view:History/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:TemplatesViewModel}">
            <view:Templates/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:HomeViewModel}">
            <view:Home></view:Home>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:WorkoutViewModel}">
            <view:Workout></view:Workout>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:SignInViewModel}">
            <view:SignIn></view:SignIn>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:RegisterViewModel}">
            <view:Register></view:Register>
        </DataTemplate>

    </Application.Resources>
</Application>
